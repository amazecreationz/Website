{
  "rules": {
    "users": {
      ".indexOn": ["permission", "crewType", "crewURL"],
      ".write": "data.child(auth.uid).child('permission').val() === 0",
      ".read": "true",
      "$userId": {
        ".read": "auth.uid === $userId || data.child('crewType').val() != null"
      }
    }
  }
}
